[run]
source = src/
omit = 
    */tests/*
    */test_*
    */__pycache__/*
    */venv/*
    */env/*
    */migrations/*
    */conftest.py
    */setup.py
    */manage.py
    src/*/example*.py
    src/*/test_*.py

branch = true
parallel = true

[report]
precision = 2
show_missing = true
skip_covered = false
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
title = H200 Intelligent Mug Positioning System - Coverage Report

[xml]
output = coverage.xml

[json]
output = coverage.json
pretty_print = true